<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.0.3</Version>
    <Authors>Anton Ganhammar</Authors>
    <RepositoryUrl>https://github.com/ganhammar/OpenIddict.AmazonDynamoDB</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/ganhammar/OpenIddict.AmazonDynamoDB</PackageProjectUrl>
    <Description>DynamoDB integration for OpenIddict</Description>
    <PackageTags>OpenIddict, DynamoDB</PackageTags>
    <PackageId>Community.OpenIddict.AmazonDynamoDB</PackageId>
    <PackageReadmeFile>../../README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.DynamoDBv2" Version="3.7.203.11" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="7.0.0" />
    <PackageReference Include="OpenIddict.Abstractions" Version="4.9.0" />
    <PackageReference Include="OpenIddict.Core" Version="4.9.0" />
  </ItemGroup>

</Project>
